{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": ["${authors}"],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}",
    "issues": "${issues}"
  },
  "license": "${license}",
  "icon": "${icon}",
  "environment": "${environment}",
  "entrypoints": {
    "client": ["megabytesme.minelights.MineLightsClient"],
    "modmenu": ["megabytesme.minelights.config.ModMenuIntegration"]
  },
  "mixins": ["minelights.mixins.json"],
  "depends": {
    "minecraft": "${minecraft}"
  },
  "suggests": {},
  "custom": {
    "modmenu": {
      "modmenu": true,
      "links": {
        "modmenu.kofi": "${kofi}",
        "modmenu.discord": "${discord}",
        "modmenu.modrinth": "${modrinth}"
      },
      "update_checker": true
    }
  }
}
